{
  "name": "Group",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "city": {
      "type":"string"
    },
    "country":{
      "type":"string"
    },
    "capacity":{
      "type":"number"
    }
  },
  "validations": [],
  "relations": {
    "account": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": ""
    },
    "refugees": {
      "type": "hasMany",
      "model": "Refugee",
      "foreignKey": ""
    },
    "questions": {
      "type": "hasMany",
      "model": "Question",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
